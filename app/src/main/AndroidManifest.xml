<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chrno.contactofragmento" >
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.CALL_PHONE"></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:supportsRtl="true">
        <activity
            android:name=".Principal"
            android:label="@string/app_name"
            android:supportsRtl="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".actividades.ActivitySecond"
            android:label="@string/title_activity_activity_second" >
        </activity>
        <activity
            android:name=".util.FormAdd"
            android:label="@string/action_add" >
        </activity>
        <activity
            android:name=".util.FormEdit"
            android:label="@string/action_edit" >
        </activity>
    </application>

</manifest>
